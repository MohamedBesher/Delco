<!DOCTYPE html>
<html lang="en" dir="rtl">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="A fully featured admin theme which can be used to build CRM, CMS, etc.">
    <meta name="author" content="Coderthemes">

    <!-- App Favicon -->
    <link rel="shortcut icon" href="/assets/images/favicon.ico">

    <!-- App title -->
    <title>Delco Admin Panel</title>

    <!-- App CSS -->
    @Styles.Render("~/Content/css")

    @Styles.Render("~/Content/kendo/css")

    <!-- HTML5 Shiv and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->

    <script src="/assets/js/modernizr.min.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Cairo" rel="stylesheet">
    @Scripts.Render("~/bundles/modernizr")

    <script src="~/assets/js/jquery.min.js"></script>





</head>
<body class="fixed-left">

    <div id="ajaxLoaderDiv" class="ripple" style="display: none;">
        <img src="~/Content/ripple.gif" class="ajax-loader" />
    </div>

    <!-- Begin page -->
    <div id="wrapper">



        <!-- Top Bar Start -->
        <div class="topbar">

            <!-- LOGO -->
            <div class="topbar-left">
                <a href="#" class="logo"><span>Delco<span>2017</span></span><i class="zmdi zmdi-layers"></i></a>
            </div>

            <!-- Button mobile view to collapse sidebar menu -->
            <div class="navbar navbar-default" role="navigation">
                <div class="container">

                    <!-- Page title -->
                    <ul class="nav navbar-nav navbar-left">
                        <li>
                            <button class="button-menu-mobile open-left">
                                <i class="zmdi zmdi-menu"></i>
                            </button>
                        </li>
                        <li>
                            <h4 class="page-title">@ViewBag.Title</h4>
                        </li>
                    </ul>
                </div><!-- end container -->
            </div><!-- end navbar -->
        </div>
        <!-- Top Bar End -->
        <!-- ========== Left Sidebar Start ========== -->
        <div class="left side-menu">
            <div class="sidebar-inner slimscrollleft">
                @Html.Partial("_LoginPartial")
                <!--- Sidemenu -->
                <div id="sidebar-menu">
                    <ul>
                        <li>
                            <a onclick="showPageLoadingSpinner()" href="~/Home" class="waves-effect">
                                <i class="zmdi zmdi-view-dashboard"></i>
                                <span>الرئيسية </span>
                            </a>


                        </li>

                        <li>
                            <a onclick="showPageLoadingSpinner()" href="@Url.Action("Index","City")" class="waves-effect">
                                <i class="fa fa-map-pin"></i>
                                <span> المدن </span>
                            </a>
                        </li>

                        <li>
                            <a href="@Url.Action("Index","PassengerNumbers")" class="waves-effect">
                                <i class="fa fa-users"></i>
                                <span> عدد الركاب </span>
                            </a>
                        </li>


                        <li class="has_sub">
                            <a href="javascript:void(0);" class="waves-effect">
                                <i class="fa fa-map-signs"></i> <span>
                                    المسارات
                                </span> <span class="menu-arrow"></span>
                            </a>
                            <ul class="list-unstyled">
                                <li><a onclick="showPageLoadingSpinner()" href="@Url.Action("Index","InternalPaths")">المسارات الداخلية</a></li>
                                <li><a onclick="showPageLoadingSpinner()" href="@Url.Action("Index","ExternalPaths")">المسارات الخارجية</a></li>

                            </ul>
                        </li>

                        <li>
                            <a onclick="showPageLoadingSpinner()" href="@Url.Action("index","RefuseResons")" class="waves-effect">
                                <i class="fa fa-window-close"></i> <span> أسباب الرفض </span>
                            </a>
                        </li>
                        <li class="has_sub">
                            <a href="javascript:void(0);" class="waves-effect">
                                <i class="fa fa-car"></i> <span>
                                    المشاوير
                                </span> <span class="menu-arrow"></span>
                            </a>
                            <ul class="list-unstyled">




                                <li><a onclick="showPageLoadingSpinner()"  href="@Url.Action("Index","Trips")">المشاوير المعروضة </a></li>
                                <li><a onclick="showPageLoadingSpinner()" href="@Url.Action("Canceled", "Trips")">المشاوير المحذوفة</a></li>
                                <li><a onclick="showPageLoadingSpinner()" href="@Url.Action("Inprogress", "Trips")">المشاوير قيد التنفيذ</a></li>
                                <li><a onclick="showPageLoadingSpinner()" href="@Url.Action("Delivered", "Trips")">المشاوير المنفذة</a></li>

                            </ul>
                        </li>

                        <li class="has_sub">
                            <a href="javascript:void(0);" class="waves-effect">
                                <i class="fa fa-cart-plus"></i> <span>
                                    الطلبات
                                </span> <span class="menu-arrow"></span>
                            </a>
                            <ul class="list-unstyled">
                                <li><a onclick="showPageLoadingSpinner()" href="@Url.Action("Index","Requests")">الطلبات المعروضة </a></li>
                                <li><a onclick="showPageLoadingSpinner()" href="@Url.Action("Canceled", "Requests")">الطلبات المحذوفة</a></li>
                                <li><a onclick="showPageLoadingSpinner()" href="@Url.Action("Inprogress", "Requests")">الطلبات قيد التنفيذ</a></li>
                                <li><a onclick="showPageLoadingSpinner()" href="@Url.Action("Delivered", "Requests")">الطلبات المنفذة</a></li>

                            </ul>
                        </li>


                        <li class="has_sub">
                            <a href="javascript:void(0);" class="waves-effect">
                                <i class="fa fa-user"></i> <span>
                                    مستخدمى النظام
                                </span> <span class="menu-arrow"></span>
                            </a>
                            <ul class="list-unstyled">
                                <li>
                                    <a onclick="showPageLoadingSpinner()" href="@Url.Action("AgentsRequest","User")">
                                        طلبات المندوبين

                                    </a>
                                </li>
                                <li><a onclick="showPageLoadingSpinner()" href="@Url.Action("Users","User")">المستخدمين </a></li>
                                <li><a onclick="showPageLoadingSpinner()" href="@Url.Action("Agents","User")">المندوبين</a></li>

                            </ul>
                        </li>

                        <li>
                            <a onclick="showPageLoadingSpinner()" href="@Url.Action("index","Setting")" class="waves-effect">
                                <i class="fa fa-cog"></i> <span> اعدادات النظام </span>
                            </a>
                        </li>



                    </ul>
                    <div class="clearfix"></div>
                </div>
                <!-- Sidebar -->
                <div class="clearfix"></div>

            </div>

        </div>
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="content-page">
            <!-- Start content -->
            <div class="content">
                <div class="container">

                    <div class="row">
                        @Html.Partial("_FlashMessage")
                        @Html.Partial("_Flash")


                        @RenderBody()
                    </div>
                    <!-- End row -->

                </div> <!-- container -->

            </div> <!-- content -->

            <footer class="footer">
                Delco © 2017 - 2018
            </footer>

        </div>
        <!-- End content-page -->

    </div>
    <script>
        var resizefunc = [];
    </script>
    @*@Scripts.Render("~/bundles/jquery")*@

    @Scripts.Render("~/bundles/js")
    @Scripts.Render("~/bundles/kendo")
    @Scripts.Render("~/bundles/flashmessage")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/unobtrusive")

    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDRGAI5VdZFjoU9UVkPxDS3xAtqvLyHl_s&libraries=places,geometry"></script>

<script src="/assets/js/jquery.blockUI.js"></script>
<script>
    function showPageLoadingSpinner() {
      
        $('#ajaxLoaderDiv').show();
      
    }

</script>
    

    @RenderSection("Scripts", false)
</body>
</html>
